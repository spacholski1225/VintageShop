@model VintageShop.Models.Arthist

@{
    ViewData["Title"] = "AllAlbums";
}

<h1>AllAlbums</h1>

<div class="table-responsive">
    <h4>Arthist</h4>
    <hr />
    <table class="table">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(m => m.Alias)</th>
                <th>@Html.DisplayFor(m => m.Alias)</th>
            </tr>
        </thead>
        <tr>
            <th>@Html.DisplayNameFor(m => m.Albums)</th>

            @foreach (var item in Model.Albums)
            {
                <th>@item.Name</th>

            }

        </tr>

    </table>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.ID">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
